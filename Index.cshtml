
@{
    ViewBag.Title = "Index";
}

<html lang="en" dir="ltr">

<head>
    <meta charset="utf-8">
    <title>Central Michigan University Computer Science Internship Application</title>
    <link rel="stylesheet" href="style.css">
</head>

<body>

    <h1>Internship Form</h1>

    <form action="Submit" method="post">

        First Name<input type="text" name="firstname" value="">
        Last Name<input type="text" name="lastname" value="">
        Student Number<input type="text" name="studentnum" value="">
        CPS 395<input type="radio" name="course" value="">
