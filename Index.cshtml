        Paid Internship<input type="radio" name="paid" value="">
        Unpaid Internship<input type="radio" name="paid" value="">
        Credits<input type="text" name="credits" value="">
        fall<input type="radio" name="semester" value="">
        spring<input type="radio" name="semester" value="">
        summer<input type="radio" name="semester" value="">
        20<input type="text" name="semester" value="">
        Course Reference Number: <input type="text" name="reference" value="">
